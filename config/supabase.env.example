# Variables d'environnement requises pour Supabase self-hosted
# Copiez ce fichier vers .env et remplissez les valeurs dans Easypanel

# Mot de passe PostgreSQL (généré automatiquement par le script)
POSTGRES_PASSWORD=ae9bf4dcb11e265619953e751be5dfc5007551a1f3538e1987c1dcf8fa935433

# Secret JWT (généré automatiquement par le script)
JWT_SECRET=035f850f68ea09404e714365d937007e021a2f30b31bd4df8b0bbb717307b0998abfba6200ef58e2c748dcb40786d2e33146c7742092b64895bf1eec32677699

# Clés API Supabase (générées au premier démarrage)
# Ces valeurs seront générées automatiquement par Supabase
# Vous pourrez les récupérer dans Supabase Studio après le premier démarrage
ANON_KEY=
SERVICE_ROLE_KEY=

# URL externe de l'API (remplacez par votre domaine si vous en avez un)
API_EXTERNAL_URL=http://kong:8000

# URL du site (remplacez par votre domaine si vous en avez un)
SITE_URL=http://localhost:3000

# URLs de redirection supplémentaires (optionnel, séparées par des virgules)
ADDITIONAL_REDIRECT_URLS=

# Configuration signup (optionnel)
DISABLE_SIGNUP=false
ENABLE_EMAIL_SIGNUP=true
ENABLE_PHONE_SIGNUP=true
ENABLE_EMAIL_AUTOCONFIRM=false
ENABLE_PHONE_AUTOCONFIRM=false

# Configuration Studio (optionnel)
STUDIO_DEFAULT_ORGANIZATION=Default Organization
STUDIO_DEFAULT_PROJECT=Default Project

# Configuration PostgREST (optionnel)
PGRST_DB_SCHEMAS=public,storage,graphql_public
PGRST_DB_EXTRA_SEARCH_PATH=public,extensions
JWT_EXP=3600

# Configuration SMS (optionnel, si vous utilisez Twilio)
SMS_PROVIDER=twilio
TWILIO_ACCOUNT_SID=
TWILIO_AUTH_TOKEN=
TWILIO_MESSAGE_SERVICE_SID=

# Configuration image proxy (optionnel)
IMGPROXY_ENABLE_WEBP_DETECTION=true
